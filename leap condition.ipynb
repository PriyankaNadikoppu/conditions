   "source": [
    "'''check whether the given input is a leap year for 4 yrs and centurians by if-else'''\n",
    "leap= int(input(\"enter the leap year:\"))\n",
    "if leap%4==0 or leap%100==0 or leap%400==0:\n",
    "    print(\"the leap year\")\n",
    "else:\n",
    "    print(\"not leap year\")"
   ]
  }
